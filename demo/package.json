{
  "name": "react-easy-transition-demo",
  "version": "1.2.3",
  "description": "Easy transitions with React and React-router",
  "engines": {
    "node": "7.2.1"
  },
  "author": {
    "name": "Antoine Stollsteiner",
    "url": "https://github.com/misterfresh"
  },
  "main": "./build/server/server/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development LOCAL=1 babel-node ./src/server",
    "test": "mocha --reporter=nyan --compilers js:babel-core/register --require ./test/test-helper.js --recursive",
    "prod": "cross-env NODE_ENV=production webpack --config ./tools/webpack.config.prod.js --progress",
    "build": "npm run clean && npm run build-client && npm run build-server && npm run upload",
    "clean": "npm run delete-build && npm run prepare-build-server && npm run prepare-build-client",
    "delete-build": "./node_modules/.bin/rimraf build",
    "prepare-build-server": "./node_modules/.bin/mkdirp build/server",
    "prepare-build-client": "./node_modules/.bin/mkdirp build/client",
    "build-client": "cross-env NODE_ENV=production webpack --config ./tools/webpack.config.build.js --debug --progress",
    "build-server": "npm run build-server-server && npm run build-server-common && npm run build-server-tools",
    "build-server-server": "babel -d ./build/server/server ./src/server -s --copy-files",
    "build-server-common": "babel -d ./build/server/common ./src/common -s --copy-files",
    "build-server-tools": "babel -d ./build/server/tools ./tools -s --copy-files",
    "local": "cross-env NODE_ENV=production node ./build/server/server/server.js",
    "upload-app": "aws s3 cp ./build/client/app.min.js s3://react-easy-transition/assets/js/app.min.js --acl public-read",
    "upload-vendors": "aws s3 cp ./build/client/vendors.min.js s3://react-easy-transition/assets/js/vendors.min.js --acl public-read",
    "upload": "npm run upload-app && npm run upload-vendors",
    "start": "node ./build/server/server/server.js"
  },
  "dependencies": {
    "aphrodite": "^1.1.0",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "helmet": "^3.1.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^3.2.3",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-thunk": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/misterfresh/react-easy-transition"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "router",
    "transition",
    "css",
    "inline"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "chokidar": "^1.6.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "happypack": "^3.0.1",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "querystring": "^0.2.0",
    "react-transform-hmr": "^1.0.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "rimraf": "^2.6.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
